{
  "runtimes": {
    "hhvmra": {
      "type": "hhvm",
      "bin": "/usr/bin/hhvm"
    },
    "hhvm": {
      "type": "hhvm",
      "bin": "/usr/bin/hhvm",
      "args": ["--no-repo-auth"]
    },
    "php7": {
      "type": "php-src",
      "bin": "/usr/bin/php-cgi7.0"
    },
    "php5": {
      "type": "php-src",
      "bin": "/usr/bin/php-cgi5.6"
    }
  },
  "targets": [
    "drupal8-no-cache",
    "drupal8-page-cache"
  ]
}
