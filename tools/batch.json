{
  "runtimes": {
    "HHVM 3.7.1": {
      "type": "hhvm",
      "bin": "/usr/bin/hhvm",
      "args": [
        "--no-repo-auth"
      ]
    },
    "HHVM 3.7.1 Repo Auth": {
      "type": "hhvm",
      "bin": "/usr/bin/hhvm",
      "args": [
        "--repo-auth"
      ]
    },
    "PHP7 (24-May-2015)": {
      "type": "php-src",
      "bin": "/usr/local/php7/bin/php-cgi"
    },
    "PHP 5.6.9": {
      "type": "php-src",
      "bin": "/usr/bin/php-cgi"
    }
  },
  "targets": [
    "drupal7",
    "drupal8-no-cache",
    "drupal8-page-cache"
  ]
}

